class Solution:
    def findMaxLength(self, nums: List[int]) -> int:
        prefix = 0
        seen = {0: -1}
        max_len = 0

        for i, num in enumerate(nums):
            prefix += 1 if num == 1 else -1

            if prefix in seen:
                max_len = max(max_len, i - seen[prefix])
            else:
                seen[prefix] = i

        return max_len
